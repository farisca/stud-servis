<table class="table table-striped" style="margin: 50px;">
	<thead>
		<tr>
			<th>{{'NOTIFICATION' | translate}}</th>
			<th>{{'DATE' | translate}}</th>
		</tr>
	</thead>
	<tr ng-repeat="notification in notifications">
		<td><a href="oglas/{{notification.job_id}}">{{notification.text}}</a></td>
		<td>{{notification.created_at | date: "dd.MM.yyyy. - hh:mm"}}</td>
	</tr>
</table>